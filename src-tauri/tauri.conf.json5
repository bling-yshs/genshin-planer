    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:11420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "windows": {
      "nsis": {
        "installerIcon": "icons/icon.ico",
        "languages": [
          "SimpChinese"
        ],
        "installMode": "perMachine"
      }
    },
    "active": true,
    "targets": [
      "nsis",
      "deb",
      "rpm",
      "dmg"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
